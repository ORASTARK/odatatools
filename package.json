{
    "name": "odatatools",
    "displayName": "odataTools",
    "description": "Tools for odata services in typescript",
    "version": "0.0.1",
    "publisher": "apazureck",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:odatatools.GetInterfaces"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "odatatools.GetInterfaces",
            "title": "oData: Get Interfaces from oData Service"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}