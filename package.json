{
    "name": "odatatools",
    "displayName": "odataTools",
    "description": "Tools for communicating with odata services in typescript",
    "version": "0.1.0",
    "publisher": "apazureck",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:odatatools.GetInterfaces",
        "onCommand:odatatools.UpdateInterfaces"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "odatatools.GetInterfaces",
            "category": "oData",
            "title": "Get Entity Interfaces from oData V4.0 Service"
        },
        {
            "command": "odatatools.UpdateInterfaces",
            "category": "oData",
            "title": "Update Entity Interfaces from oData V4 Service"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "node-rest-client": "2.0.1"
    },
    "author": {
        "email": "info@pazureck.de",
        "name": "Andreas Pazureck"
    },
    "keywords": [
        "oData"
    ],
    "preview": true,
    "galleryBanner": {
        "color": "white",
        "theme": "light"
    },
    "license": "MIT",
    "bugs": "https://github.com/apazureck/odatatools/issues",
    "repository": "https://github.com/apazureck/odatatools",
    "icon": "images/odata.png"
}